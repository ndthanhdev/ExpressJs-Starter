{
  "name": "ExpressJs-Starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "npm run eslint && npm run watch",
    "watch": "nodemon -e js --watch ./ --ignore node_modules --exec \"npm run serve\"",
    "serve": "node --inspect server.js",
    "test": "jest",
    "eslint": "eslint **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thanhtdt/ExpressJs-Starter.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thanhtdt/ExpressJs-Starter/issues"
  },
  "homepage": "https://github.com/thanhtdt/ExpressJs-Starter#readme",
  "dependencies": {
    "body-parser": "^1.17.2",
    "compression": "^1.7.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.15.4",
    "morgan": "^1.8.2"
  },
  "devDependencies": {
    "eslint": "^4.4.1",
    "eslint-plugin-jest": "^20.0.3",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "supertest": "^3.0.0"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "rules": {
      "comma-dangle": 0,
      "consistent-return": 0,
      "no-param-reassign": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 0,
      "no-console": 0,
      "no-plusplus": 0,
      "no-unused-expressions": 0
    },
    "env": {
      "es6": true,
      "node": true,
      "jest": true
    },
    "plugins": [
      "jest"
    ]
  }
}
